  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "pickaxe no args",
      "program": "${workspaceFolder}/index.js",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "babel-node"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "pickaxe ls",
      "program": "${workspaceFolder}/index.js",
      "args": [
        "ls"
      ],
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "babel-node"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "pickaxe reset-all",
      "program": "${workspaceFolder}/index.js",
      "args": [
        "reset-all"
      ],
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "babel-node"
      ]
    }
  ]
}